{
  "name": "mjCamera",
  "version": "0.0.1",
  "summary": "A simple camera library",
  "homepage": "https://github.com/blackho1e/mjCamera",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Minju Kang": "blackdole@naver.com"
  },
  "source": {
    "git": "https://github.com/blackho1e/mjCamera.git",
    "tag": "0.0.1"
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "source_files": "Classes/**/*.swift",
  "resources": [
    "Classes/Assets.xcassets",
    "Classes/**/*.xib"
  ],
  "subspecs": [
    {
      "name": "Localization",
      "subspecs": [
        {
          "name": "en",
          "ios": {
            "resources": "Classes/en.lproj",
            "preserve_paths": "Classes/en.lproj"
          }
        },
        {
          "name": "ko",
          "ios": {
            "resources": "Classes/ko.lproj",
            "preserve_paths": "Classes/ko.lproj"
          }
        }
      ]
    }
  ]
}
